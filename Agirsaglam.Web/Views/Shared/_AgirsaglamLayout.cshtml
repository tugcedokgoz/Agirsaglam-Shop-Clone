<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Agirsaglam.Web</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Agirsaglam.Web.styles.css" asp-append-version="true" />



    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Ağırsağlam Mağaza</title>
    <meta name="description" content="description">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Favicon -->
    <link rel="shortcut icon" href="~/agirsaglamui/assets/images/favicon.png" />
   
    <!-- Plugins CSS -->
    <link rel="stylesheet" href="~/agirsaglamui/assets/css/plugins.css">
    <!-- Bootstap CSS -->
    <link rel="stylesheet" href="~/agirsaglamui/assets/css/bootstrap.min.css">
    <!-- Main Style CSS -->
    <link rel="stylesheet" href="~/agirsaglamui/assets/css/style.css">
    <link rel="stylesheet" href="~/agirsaglamui/assets/css/responsive.css">
</head>
<body class="template-index belle template-index-belle">
    <div id="pre-loader">
        <img src="~/agirsaglamui/assets/images/loader.gif" alt="Loading..." />
    </div>
    <div class="pageWrapper">
        <!--Search Form Drawer-->
        <div class="search">
            <div class="search__form">
                <form class="search-bar__form" action="#">
                    <button class="go-btn search__button" type="submit"><i class="icon anm anm-search-l"></i></button>
                    <input class="search__input" type="search" name="q" value="" placeholder="Search entire store..." aria-label="Search" autocomplete="off">
                </form>
                <button type="button" class="search-trigger close-btn"><i class="anm anm-times-l"></i></button>
            </div>
        </div>
        <!--End Search Form Drawer-->
        <!--Top Header-->
        <div class="top-header">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-10 col-sm-8 col-md-5 col-lg-4">
                    </div>
                    <div class="col-sm-4 col-md-4 col-lg-4 d-none d-lg-none d-md-block d-lg-block">
                        <div class="text-center"><p class="top-header_middle-text">AĞIRSAĞLAM GÜVENLİ, KALİTELİ VE HIZLIDIR</p></div>
                    </div>
                    <div class="col-2 col-sm-4 col-md-3 col-lg-4 text-right">
                        <span class="user-menu d-block d-lg-none"><i class="anm anm-user-al" aria-hidden="true"></i></span>
                        <ul class="customer-links list-inline">
                            <li><a href="https://localhost:7203/Account/Login">Giriş Yap</a></li>
                            <li><a href="https://localhost:7203/Kullanici/Home/register">Kayıt Ol</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>


        <div id="layoutSidenav_content">
            <main>
                @RenderBody()
            </main>
            <!--Footer-->
            <footer id="footer">
                <div class="newsletter-section bg-light">
                    <div class="container bg-light">
                        <div class="row">
                            <div class="col-12 d-flex justify-content-center align-items-center">
                                <div class="footer-social">
                                    <ul class="list--inline site-footer__social-icons social-icons">
                                        <li><a class="social-icons__link" href="#" target="_blank" title="Facebook"><i class="icon icon-facebook"></i></a></li>
                                        <li><a class="social-icons__link" href="#" target="_blank" title="Instagram"><i class="icon icon-instagram"></i> <span class="icon__fallback-text">Instagram</span></a></li>
                                        <li><a class="social-icons__link" href="#" target="_blank" title="YouTube"><i class="icon icon-youtube"></i> <span class="icon__fallback-text">YouTube</span></a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="site-footer bg-light">
                    <div class="container">
                        <!--Footer Links-->
                        <div class="py-5 my-6">
                            <ul class="nav justify-content-center border-bottom pb-5 mb-5">
                                <li class="nav-item ">
                                    <a class="nav-link px-2 text-muted " href="https://localhost:7203/Kullanici/Home/aboutus">Hakkımızda</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link px-2 text-muted" href="https://localhost:7203/Kullanici/Home/userguide">Kullanım Koşulları</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link px-2 text-muted" href="https://localhost:7203/Kullanici/Home/privacypolicy">Gizlilik</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link px-2 text-muted" href="https://localhost:7203/Kullanici/Home/terms">Satış Sözleşmeleri</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link px-2 text-muted" href="https://localhost:7203/Kullanici/Home/returnpolicy">İade Politikası</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link px-2 text-muted" href="https://localhost:7203/Kullanici/Home/blog">Mağaza Blog</a>
                                </li>
                            </ul>
                        </div>
                        <hr>

                    </div>
                </div>

            </footer>
        </div>

        <!--End Footer-->
        <!--Scoll Top-->
        <span id="site-scroll"><i class="icon anm anm-angle-up-r"></i></span>
        <!--End Scoll Top-->
        <!-- Including Jquery -->
        <script src="~/agirsaglamui/assets/js/vendor/jquery-3.3.1.min.js"></script>
        <script src="~/agirsaglamui/assets/js/vendor/modernizr-3.6.0.min.js"></script>
        <script src="~/agirsaglamui/assets/js/vendor/jquery.cookie.js"></script>
        <script src="~/agirsaglamui/assets/js/vendor/wow.min.js"></script>
        <!-- Including Javascript -->
        <script src="~/agirsaglamui/assets/js/bootstrap.min.js"></script>
        <script src="~/agirsaglamui/assets/js/plugins.js"></script>
        <script src="~/agirsaglamui/assets/js/popper.min.js"></script>
        <script src="~/agirsaglamui/assets/js/lazysizes.js"></script>
        <script src="~/agirsaglamui/assets/js/main.js"></script>
    </div>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
